{
  "name": "omflow-svelte",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "last 1 chrome versions"
  ],
  "scripts": {
    "start": "concurrently \"npm run server:prod\" \"npm run client:prod\"",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "server": "nodemon server.js --ignore src/",
    "client": "parcel src/index.html public/",
    "server:prod": "node server.js",
    "client:prod": "npm run build && npm run serve",
    "build": "parcel build src/index.html",
    "serve": "parcel src/index.html"
  },
  "devDependencies": {
    "sass": "^1.23.0",
    "svelte": "^3.29.4",
    "parcel-plugin-svelte": "^4.0.4"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.9.1",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^1.12.0",
    "mapbox-gl-geocoder": "^2.0.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "parcel-bundler": "^1.12.4",
    "svelte-spa-router": "^3.0.4"
  }
}
